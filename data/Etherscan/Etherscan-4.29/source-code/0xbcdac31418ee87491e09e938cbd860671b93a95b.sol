    "src/HotPotDevice/HotPotDevice.sol": {
      "content": "// SPDX-License-Identifier: MIT\n\npragma solidity ^0.8.20;\n\nimport {Ownable} from \"@openzeppelin/contracts/access/Ownable.sol\";\nimport {IERC20} from \"@openzeppelin/contracts/interfaces/IERC20.sol\";\n\nimport {EventsAndErrors} from \"./EventsAndErrors.sol\";\nimport {ERC721A} from \"./lib/erc721a/contracts/ERC721A.sol\";\n\n/**\n * @title HotPot device contract implementation.\n */\ncontract HotPotDevice is\n    Ownable,\n    ERC721A,\n    EventsAndErrors\n{\n    // max supply\n    uint256 public constant MAX_SUPPLY = 20000;\n\n    // mint price in USDT\n    uint256 public priceUSDT;\n\n    // mint price in POT\n    uint256 public pricePOT;\n\n    // USDT\n    address public constant USDT = 0xdAC17F958D2ee523a2206206994597C13D831ec7;\n\n    // POT token\n    address public constant POT = 0x6a681AFA304aB6F19352715cCD0A0478Ee39f6fe;\n\n    // base token URI\n    string public baseURI;\n\n    // status\n    Status public status;\n\n    // maximum batch mint number\n    uint256 internal constant _maxBatchNum = 17;\n\n    // mapping token id to packed meta id\n    mapping(uint256 => uint256) internal _packedMetaIds;\n    \n    // random indices\n    uint256[MAX_SUPPLY] internal _randIndices;\n\n    // status pipeline\n    enum Status {\n        INIT,\n        OPEN,\n        PAUSED\n    }\n\n    // payment token type\n    enum PaymentType {\n        POT,\n        USDT\n    }\n\n    /**\n     * @notice Make sure that the caller is user.\n     */\n    modifier callerIsUser() {\n        if (tx.origin != _msgSender()) revert CallerIsNotUser();\n\n        _;\n    }\n\n    /**\n     * @notice Constructor.\n     * @param priceUSDT_  The price in USDT\n     * @param pricePOT_  The price in POT\n     * @param baseURI_  The base token URI\n     * @param status_ The initial status\n     */\n    constructor(\n        uint256 priceUSDT_,\n        uint256 pricePOT_,\n        string memory baseURI_,\n        Status status_\n    ) ERC721A(\"HotPot Device\", \"HotPotDevice\") {\n        priceUSDT = priceUSDT_;\n        pricePOT= pricePOT_;\n\n        baseURI = baseURI_;\n        status = status_;\n    }\n\n    /**\n     * @notice Mint tokens.\n     * @param quantity The quantity of tokens to be minted\n     * @param paymentType The type of the token used for payment\n     */\n    function mint(\n        uint256 quantity,\n        PaymentType paymentType\n    ) external callerIsUser {\n        if (status != Status.OPEN) revert MintNotEnabled();\n\n        _checkAndMint(_msgSender(), quantity, paymentType);\n    }\n\n    /**\n     * @notice Set the current status.\n     * @param status_ The current status\n     */\n    function setStatus(Status status_) external onlyOwner {\n        status = status_;\n    }\n\n    /**\n     * @notice Set the base token URI.\n     * @param baseURI_ The base token URI\n     */\n    function setBaseURI(string calldata baseURI_) external onlyOwner {\n        baseURI = baseURI_;\n    }\n\n    /**\n     * @notice Set the price in USDT.\n     * @param priceUSDT_ The price in USDT\n     */\n    function setPriceInUSDT(uint256 priceUSDT_) external onlyOwner {\n        priceUSDT = priceUSDT_;\n    }\n\n    /**\n     * @notice Set the price in POT.\n     * @param pricePOT_ The price in POT\n     */\n    function setPriceInPOT(uint256 pricePOT_) external onlyOwner {\n        pricePOT = pricePOT_;\n    }\n\n    /**\n     * @notice Withdraw balance by the specified token.\n     * @param to The recipient address\n     * @param token The target token\n     */\n    function withdraw(address to, address token) external onlyOwner {\n        uint256 balance = IERC20(token).balanceOf(address(this));\n        if (balance == 0) revert InsufficientBalance();\n\n        IERC20(token).transfer(to, balance);\n    }\n\n    /**\n     * @notice Check and mint `quantity` of tokens to the specified account.\n     * @param to The specified recipient address\n     * @param quantity The quantity of tokens to be minted\n     * @param paymentType The type of the token used for payment\n     */\n    function _checkAndMint(\n        address to,\n        uint256 quantity,\n        PaymentType paymentType\n    ) internal {\n        if (quantity > _maxBatchNum) revert MaxBatchNumExceeded();\n        if (totalSupply() + quantity > MAX_SUPPLY) revert MaxSupplyExceeded();\n\n        _paymentHandler(to, quantity, paymentType);\n\n        _safeMint(to, quantity);\n        _setMetaIds(quantity);\n    }\n\n    /**\n     * @notice Handle payment.\n     * @param account The payment address\n     * @param quantity The quantity of tokens to be minted\n     * @param paymentType The type of the token used for payment\n     */\n    function _paymentHandler(\n        address account,\n        uint256 quantity,\n        PaymentType paymentType\n    ) internal {\n        if (paymentType == PaymentType.USDT) {\n            _paymentByToken(account, priceUSDT, quantity, IERC20(USDT));\n        } else {\n            if (pricePOT == 0) revert POTPaymentNotEnabled();\n\n            _paymentByToken(account, pricePOT, quantity, IERC20(POT));\n        }\n    }\n\n    /**\n     * @notice Handle payment by the given token.\n     * @param account The payment address\n     * @param price The unit price\n     * @param quantity The quantity of tokens to be minted\n     * @param paymentToken The type of the token used for payment\n     */\n    function _paymentByToken(\n        address account,\n        uint256 price,\n        uint256 quantity,\n        IERC20 paymentToken\n    ) internal {\n        uint256 totalPrice = price * quantity;\n\n        paymentToken.transferFrom(account, address(this), totalPrice);\n    }\n\n    /**\n     * @notice Set the meta ids for the specified tokens.\n     * @param quantity The quantity of the tokens\n     */\n    function _setMetaIds(\n        uint256 quantity\n    ) internal {\n        uint256 preTotalMinted = _totalMinted() - quantity;\n        uint256 startTokenId = preTotalMinted + _startTokenId();\n\n        // set the meta id into the extra data for the starting token\n        _setExtraDataAt(startTokenId, uint24(_getRandomMetaId(preTotalMinted)));\n \n        if (quantity > 1) {\n             uint256 packedMetaId;\n\n            for(uint256 i = 1; i < quantity; i++) {\n                packedMetaId |= _getRandomMetaId(preTotalMinted + i) << (16 * (i - 1));\n            }\n\n            _packedMetaIds[startTokenId] = packedMetaId;\n        }\n    }\n\n    /**\n     * @notice Get the meta id of the given token.\n     * @param tokenId The id of the given token\n     * @return metaId The meta id of the given token\n     */\n    function _getMetaId(uint256 tokenId) internal view returns (uint256) {\n        uint256 metaId = _getMetaIdFromExtraData(tokenId);\n        if (metaId > 0 ) {\n            return metaId;\n        }\n\n        return _getMetaIdFromPackedMetaIds(tokenId);\n    }\n\n    /**\n     * @notice Get the meta id of the given token from the extra data\n     * @param tokenId The id of the given token\n     */\n    function _getMetaIdFromExtraData(uint256 tokenId) internal view returns (uint256) {\n        return _ownershipAt(tokenId).extraData;\n    }\n\n    /**\n     * @notice Get the meta id of the given token from the packed meta ids.\n     * @param tokenId The id of the given token\n     * @return metaId The meta id of the given token\n     */\n    function _getMetaIdFromPackedMetaIds(uint256 tokenId) internal view returns (uint256) {\n        uint256 index = _getPreNonEmptyExtraDataIndex(tokenId);\n\n        return uint16(_packedMetaIds[index] >> (16 * (tokenId - index - 1)));\n    }\n\n    /**\n     * @notice Override the super._startTokenId() implementation.\n     * @return startTokenId The starting id of tokens\n     */\n    function _startTokenId() internal view virtual override returns (uint256) {\n        return 1;\n    }\n\n    /**\n     * @notice See {IERC721Metadata-tokenURI}.\n     */\n    function tokenURI(\n        uint256 tokenId\n    ) public view virtual override returns (string memory) {\n        if (!_exists(tokenId)) revert TokenDoesNotExist();\n\n        return string.concat(baseURI, _toString(_getMetaId(tokenId)));\n    }\n\n    /**\n     * @notice Get the random meta id.\n     * @param totalMinted The total number of the minted tokens\n     * @return metaId Meta id\n     */\n    function _getRandomMetaId(uint256 totalMinted) internal returns (uint256) {\n        unchecked {\n            uint256 remain = MAX_SUPPLY - totalMinted;\n            uint256 pos = _getRandom(totalMinted) % remain;\n            uint256 val = _randIndices[pos] == 0 ? pos : _randIndices[pos];\n            _randIndices[pos] = _randIndices[remain - 1] == 0 ? remain - 1 : _randIndices[remain - 1];\n            \n            return val + 1;\n        }\n    }\n\n    /**\n     * @notice Generate a random number by on-chain randomness.\n     * @param totalMinted The total number of the minted tokens\n     * @return random Generated random number\n     */\n    function _getRandom(uint256 totalMinted) internal view returns (uint256) {\n        unchecked {\n            return uint256(keccak256(abi.encodePacked(\n                block.prevrandao,\n                block.coinbase,\n                totalMinted,\n                tx.origin\n            )));\n        }\n    }\n\n    /**\n     * @notice Get the index of the previous token which contains the non empty extra data.\n     * Note: This method always returns the expected result for the contract.\n     * @param tokenId The id of the given token\n     * @return index The index of the previous token which contains the non empty extra data\n     */\n    function _getPreNonEmptyExtraDataIndex(\n        uint256 tokenId\n    ) internal view returns (uint256) {\n        TokenOwnership memory ownership;\n\n        for (uint256 i = tokenId - 1; i >= _startTokenId(); i--) {\n            ownership = _ownershipAt(i);\n\n            if (ownership.extraData != 0) {\n                return i;\n            }\n        }\n\n        return 0; // never happen\n    }\n\n    /**\n     * @notice Query all tokens of the given owner.\n     * @param owner The owner\n     * @return tokenIds The ids of tokens of the owner\n     */\n    function tokensOfOwner(\n        address owner\n    ) external view returns (uint256[] memory) {\n        uint256 tokenIdsIdx;\n        address currOwnershipAddr;\n\n        uint256 tokenIdsLength = balanceOf(owner);\n        uint256[] memory tokenIds = new uint256[](tokenIdsLength);\n\n        TokenOwnership memory ownership;\n        for (uint256 i = _startTokenId(); tokenIdsIdx != tokenIdsLength; ++i) {\n            ownership = _ownershipAt(i);\n\n            if (ownership.burned) {\n                continue;\n            }\n\n            if (ownership.addr != address(0)) {\n                currOwnershipAddr = ownership.addr;\n            }\n\n            if (currOwnershipAddr == owner) {\n                tokenIds[tokenIdsIdx++] = i;\n            }\n        }\n\n        return tokenIds;\n    }\n}\n"
    },
    "src/HotPotDevice/lib/erc721a/contracts/ERC721A.sol": {
      "content": "// SPDX-License-Identifier: MIT\n// ERC721A Contracts v4.2.3\n// Creator: Chiru Labs\n\npragma solidity ^0.8.4;\n\nimport './IERC721A.sol';\n\n/**\n * @dev Interface of ERC721 token receiver.\n */\ninterface ERC721A__IERC721Receiver {\n    function onERC721Received(\n        address operator,\n        address from,\n        uint256 tokenId,\n        bytes calldata data\n    ) external returns (bytes4);\n}\n\n/**\n * @title ERC721A\n *\n * @dev Implementation of the [ERC721](https://eips.ethereum.org/EIPS/eip-721)\n * Non-Fungible Token Standard, including the Metadata extension.\n * Optimized for lower gas during batch mints.\n *\n * Token IDs are minted in sequential order (e.g. 0, 1, 2, 3, ...)\n * starting from `_startTokenId()`.\n *\n * Assumptions:\n *\n * - An owner cannot have more than 2**64 - 1 (max value of uint64) of supply.\n * - The maximum token ID cannot exceed 2**256 - 1 (max value of uint256).\n */\ncontract ERC721A is IERC721A {\n    // Bypass for a `--via-ir` bug (https://github.com/chiru-labs/ERC721A/pull/364).\n    struct TokenApprovalRef {\n        address value;\n    }\n\n    // =============================================================\n    //                           CONSTANTS\n    // =============================================================\n\n    // Mask of an entry in packed address data.\n    uint256 private constant _BITMASK_ADDRESS_DATA_ENTRY = (1 << 64) - 1;\n\n    // The bit position of `numberMinted` in packed address data.\n    uint256 private constant _BITPOS_NUMBER_MINTED = 64;\n\n    // The bit position of `numberBurned` in packed address data.\n    uint256 private constant _BITPOS_NUMBER_BURNED = 128;\n\n    // The bit position of `aux` in packed address data.\n    uint256 private constant _BITPOS_AUX = 192;\n\n    // Mask of all 256 bits in packed address data except the 64 bits for `aux`.\n    uint256 private constant _BITMASK_AUX_COMPLEMENT = (1 << 192) - 1;\n\n    // The bit position of `startTimestamp` in packed ownership.\n    uint256 private constant _BITPOS_START_TIMESTAMP = 160;\n\n    // The bit mask of the `burned` bit in packed ownership.\n    uint256 private constant _BITMASK_BURNED = 1 << 224;\n\n    // The bit position of the `nextInitialized` bit in packed ownership.\n    uint256 private constant _BITPOS_NEXT_INITIALIZED = 225;\n\n    // The bit mask of the `nextInitialized` bit in packed ownership.\n    uint256 private constant _BITMASK_NEXT_INITIALIZED = 1 << 225;\n\n    // The bit position of `extraData` in packed ownership.\n    uint256 private constant _BITPOS_EXTRA_DATA = 232;\n\n    // Mask of all 256 bits in a packed ownership except the 24 bits for `extraData`.\n    uint256 private constant _BITMASK_EXTRA_DATA_COMPLEMENT = (1 << 232) - 1;\n\n    // The mask of the lower 160 bits for addresses.\n    uint256 private constant _BITMASK_ADDRESS = (1 << 160) - 1;\n\n    // The maximum `quantity` that can be minted with {_mintERC2309}.\n    // This limit is to prevent overflows on the address data entries.\n    // For a limit of 5000, a total of 3.689e15 calls to {_mintERC2309}\n    // is required to cause an overflow, which is unrealistic.\n    uint256 private constant _MAX_MINT_ERC2309_QUANTITY_LIMIT = 5000;\n\n    // The `Transfer` event signature is given by:\n    // `keccak256(bytes(\"Transfer(address,address,uint256)\"))`.\n    bytes32 private constant _TRANSFER_EVENT_SIGNATURE =\n        0xddf252ad1be2c89b69c2b068fc378daa952ba7f163c4a11628f55a4df523b3ef;\n\n    // =============================================================\n    //                            STORAGE\n    // =============================================================\n\n    // The next token ID to be minted.\n    uint256 private _currentIndex;\n\n    // The number of tokens burned.\n    uint256 private _burnCounter;\n\n    // Token name\n    string private _name;\n\n    // Token symbol\n    string private _symbol;\n\n    // Mapping from token ID to ownership details\n  