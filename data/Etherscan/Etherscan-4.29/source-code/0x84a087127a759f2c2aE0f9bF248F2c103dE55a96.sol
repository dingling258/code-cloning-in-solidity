    "contracts/AntiBot.sol": {
      "content": "// SPDX-License-Identifier: UNLICENSED\npragma solidity ^0.8.0;\n\n/**\n *  ____  _______  _______           _     \n * |  _ \\| ____\\ \\/ /_   _|__   ___ | |___ \n * | | | |  _|  \\  /  | |/ _ \\ / _ \\| / __|\n * | |_| | |___ /  \\  | | (_) | (_) | \\__ \\\n * |____/|_____/_/\\_\\ |_|\\___/ \\___/|_|___/\n *\n * This smart contract was created effortlessly using the DEXTools Token Creator.\n * \n * ðŸŒ Website: https://www.dextools.io/\n * ðŸ¦ Twitter: https://twitter.com/DEXToolsApp\n * ðŸ’¬ Telegram: https://t.me/DEXToolsCommunity\n * \n * ðŸš€ Unleash the power of decentralized finances and tokenization with DEXTools Token Creator. Customize your token seamlessly. Manage your created tokens conveniently from your user panel - start creating your dream token today!\n */\nimport { Ownable } from \"@openzeppelin/contracts/access/Ownable.sol\";\nimport { EnumerableSet } from \"@openzeppelin/contracts/utils/structs/EnumerableSet.sol\";\n\ncontract AntiBot is Ownable {\n    using EnumerableSet for EnumerableSet.AddressSet;\n\n    uint256 public constant VERSION = 1;\n    uint256 public timeLimit = 2 days; // Limit time add/remove blacklist\n    uint256 public addressLimit = 10; // Limit address allow to blacklist\n\n    struct ContractValues {\n        address owner;\n        uint256 created;\n        uint256 limit;\n        uint256 addressLimit;\n        uint256 blacklisted;\n    }\n\n    mapping(address => ContractValues) private _contracts;\n    mapping(address => EnumerableSet.AddressSet) private _blacklisted;\n\n    event TimeLimitUpdated(uint256 limit);\n    event AddressLimitUpdated(uint256 limit);\n    event TokenOwnerUpdated(address token, address owner);\n\n    function setTokenOwner(address owner_) external {\n        _contracts[_msgSender()] = ContractValues(\n            owner_,\n            block.timestamp,\n            timeLimit,\n            addressLimit,\n            0\n        );\n    }\n\n    function updateTokenOwner(address token, address newOwner) external {\n        require(\n            _contracts[token].owner == _msgSender(),\n            \"Only the token owner allowed\"\n        );\n        _contracts[token].owner = newOwner;\n        emit TokenOwnerUpdated(token, newOwner);\n    }\n\n    function onPreTransferCheck(address from, address to) external view {\n        require(\n            !_blacklisted[_msgSender()].contains(from) &&\n                !_blacklisted[_msgSender()].contains(to),\n            \"This address is blacklisted\"\n        );\n    }\n\n    function addToBlacklist(\n        address owner_,\n        address[] calldata addresses\n    ) external {\n        require(\n            _contracts[_msgSender()].owner == owner_,\n            \"Only the owner can add to blacklist\"\n        );\n        require(\n            block.timestamp <\n                _contracts[_msgSender()].created +\n                    _contracts[_msgSender()].limit,\n            \"Can not perfom this operation\"\n        );\n        for (uint256 i = 0; i < addresses.length; i++) {\n            if (!_blacklisted[_msgSender()].contains(addresses[i])) {\n                _blacklisted[_msgSender()].add(addresses[i]);\n                _contracts[_msgSender()].blacklisted += 1;\n            }\n        }\n        require(\n            _contracts[_msgSender()].blacklisted <=\n                _contracts[_msgSender()].addressLimit,\n            \"blacklisted more than allowed\"\n        );\n    }\n\n    function removeFromBlacklist(address owner_, address account) external {\n        require(\n            _contracts[_msgSender()].owner == owner_,\n            \"Only the owner can remove from blacklist\"\n        );\n        require(\n            block.timestamp <\n                _contracts[_msgSender()].created +\n                    _contracts[_msgSender()].limit,\n            \"Can not perfom this operation\"\n        );\n        if (_blacklisted[_msgSender()].contains(account)) {\n            _blacklisted[_msgSender()].remove(account);\n            _contracts[_msgSender()].blacklisted -= 1;\n        }\n    }\n\n    function getBlacklistStatus(\n        address token\n    ) external view returns (uint256, uint256, uint256, uint256) {\n        return (\n            _contracts[token].created,\n            _contracts[token].limit,\n            _contracts[token].addressLimit,\n            _contracts[token].blacklisted\n        );\n    }\n\n    function getBlacklist(\n        address token\n    ) external view returns (address[] memory) {\n        uint256 length = _blacklisted[token].length();\n\n        address[] memory blacklist = new address[](length);\n\n        for (uint256 i; i < length; i++) {\n            blacklist[i] = _blacklisted[token].at(i);\n        }\n\n        return blacklist;\n    }\n\n    function isBlacklisted(\n        address token,\n        address account\n    ) external view returns (bool) {\n        return _blacklisted[token].contains(account);\n    }\n\n    function setTimeLimit(uint256 newLimit_) public onlyOwner {\n        require(\n            newLimit_ <= 10 days,\n            \"time limit must be less or equal 10 days\"\n        );\n        require(\n            newLimit_ >= 1 days,\n            \"time limit must be greater or equal 1 day\"\n        );\n        timeLimit = newLimit_;\n        emit TimeLimitUpdated(newLimit_);\n    }\n\n    function setAddressLimit(uint256 newLimit_) public onlyOwner {\n        require(newLimit_ <= 20, \"addr limit must be less or equal 20\");\n        require(newLimit_ >= 1, \"addr limit must be greater or equal 1\");\n        addressLimit = newLimit_;\n        emit AddressLimitUpdated(newLimit_);\n    }\n}\n"
    }
