            _callOptionalReturn(token, approvalCall);
        }
    }

    /**
     * @dev Use a ERC-2612 signature to set the `owner` approval toward `spender` on `token`.
     * Revert on invalid signature.
     */
    function safePermit(
        IERC20Permit token,
        address owner,
        address spender,
        uint256 value,
        uint256 deadline,
        uint8 v,
        bytes32 r,
        bytes32 s
    ) internal {
        uint256 nonceBefore = token.nonces(owner);
        token.permit(owner, spender, value, deadline, v, r, s);
        uint256 nonceAfter = token.nonces(owner);
        require(nonceAfter == nonceBefore + 1, "SafeERC20: permit did not succeed");
    }

    /**
     * @dev Imitates a Solidity high-level call (i.e. a regular function call to a contract), relaxing the requirement
     * on the return value: the return value is optional (but if data is returned, it must not be false).
     * @param token The token targeted by the call.
     * @param data The call data (encoded using abi.encode or one of its variants).
     */
    function _callOptionalReturn(IERC20 token, bytes memory data) private {
        // We need to perform a low level call here, to bypass Solidity's return data size checking mechanism, since
        // we're implementing it ourselves. We use {Address-functionCall} to perform this call, which verifies that
        // the target address contains contract code and also asserts for success in the low-level call.

        bytes memory returndata = address(token).functionCall(data, "SafeERC20: low-level call failed");
        require(returndata.length == 0 || abi.decode(returndata, (bool)), "SafeERC20: ERC20 operation did not succeed");
    }

    /**
     * @dev Imitates a Solidity high-level call (i.e. a regular function call to a contract), relaxing the requirement
     * on the return value: the return value is optional (but if data is returned, it must not be false).
     * @param token The token targeted by the call.
     * @param data The call data (encoded using abi.encode or one of its variants).
     *
     * This is a variant of {_callOptionalReturn} that silents catches all reverts and returns a bool instead.
     */
    function _callOptionalReturnBool(IERC20 token, bytes memory data) private returns (bool) {
        // We need to perform a low level call here, to bypass Solidity's return data size checking mechanism, since
        // we're implementing it ourselves. We cannot use {Address-functionCall} here since this should return false
        // and not revert is the subcall reverts.

        (bool success, bytes memory returndata) = address(token).call(data);
        return
            success && (returndata.length == 0 || abi.decode(returndata, (bool))) && Address.isContract(address(token));
    }
}

// File: contracts/MCDStaking.sol


pragma solidity ^0.8.18;








interface NFT {
    function transferFrom(address sender, address receiver, uint256 tokenID) external;
}

contract MCDStaking is ERC721Holder, Ownable, ReentrancyGuard {
    
    using Strings for uint256;
    using SafeMath for uint256;
    using SafeERC20 for IERC20;

    address public immutable mcd = 0x13E0c7f86E28372D5FE7d9F002045e10eB288CF7;
    address public immutable treasury = 0xA682182447D462dc980a4d0CD0e81D0c4B6a235C;

    uint256 private immutable genesisMigrationDate = 1710777600;
    uint256 private constant genesisLockPeriod = 150 days;

    uint256 private constant mcdPercent = 70;
    uint256 private constant xmcdPercent = 30;
    uint256 private constant divider = 100;

    uint256 private constant timeStep = 1 days;
    uint256 private constant vestingPeriod = 90 days;

    uint256 public immutable stakingStart = 1711382400;
    uint256 public lastLiquidityRelease;

    uint256 public tokenEmission = 300000e18;
    uint256 public totalCollection = 2;
    uint256 public totalHashpower;

    struct StakeInfo {
        uint256 collectionID;
        address nftAddr;
        uint256 tokenID;
        uint256 stakeTime;
        uint256 hashpower;
        uint256 lastClaimTime;
        bool staked;
    }

    struct UserData {
        uint256 xMCD;
        uint256 hashpower;
    }

    mapping(address=>StakeInfo[]) public stakeInfo;
    mapping(address=>UserData) public userData;
    mapping(address=>bool) private admin;
    mapping(address=>uint256) public nftHashpower;
    mapping(uint256=>address) public nftContract;
    mapping(address=>uint256) public genesisMigratedToken;
    mapping(address=>mapping(uint256=>bool)) public stakedNFT;

    event Stake(address user, address nftAddr, uint256 tokenID, uint256 timestamp);
    event BatchStake(address user, address nftAddr, uint256[] tokenIDs, uint256 timestamp);
    event Unstake(address user, address nftAddr, uint256 tokenID, uint256 timestamp);
    event ClaimSingleNFTYield(address user, address nftAddr, uint256 tokenID, uint256 timestamp, uint256 totalYield);
    event ClaimAll(address user, uint256 timestamp, uint256 mcdEarned, uint256 xmcdEarned);
    event TransferXMCD(address sender, address receiver, uint256 transferAmount, uint256 timestamp);
    event DebitXMCD(address spender, uint256 spendAmount, uint256 timestamp);
    event CreditXMCD(address adminAddr, address receiver, uint256 creditAmount, uint256 timestamp);
    event GenesisStakeMigrate(address adminAddr, address holderWallet, uint256 tokenID, uint256 startTime, uint256 lastClaim);
    event GenesisStakeBatchMigrate(address adminAddr, address[] wallet, uint256[] tokenIDs, uint256 startTime, uint256 lastClaim);
    event GenesisTokenMigrate(address adminAddr, address receiver, uint256 amount);
    event GenesisTokenBatchMigrate(address adminAddr, address[] receiver, uint256[] amount);
    event GenesisTokenClaim(address user, uint256 mcdEarned, uint256 xmcdEarned, uint256 timestamp);
    event ReleaseToTreasury(address caller, uint256 unreleasedDays, uint256 releasedAmount, uint256 timestamp);
    event SetTokenEmission(address adminAddr, uint256 newEmission, uint256 timestamp);
    event AddNFTContract(address adminAddr, uint256 collectionID, address nftAddr, uint256 timestamp);
    event ManageAdmin(address newAdminAddr, bool state, uint256 timestamp);
    event UpdateNFTHashpower(address adminAddr, uint256 oldPower, uint256 newPower, uint256 timestamp);
    event SetGenesisContractAddr(address adminAddr, address genesisAddr, uint256 timestamp);

    constructor(
    ) {
        lastLiquidityRelease = block.timestamp;
        nftContract[1] = 0xeB248E38eC67DbE9Cc3DC82bB5eD6d1681939CD8;
        nftHashpower[nftContract[1]] = 20;
    }

    function stake(uint256 _collectionID, uint256 _tokenID) external nonReentrant {
        address nftAddr = nftContract[_collectionID];
        require(nftAddr != address(0), "Cannot be null address.");
        require(!stakedNFT[nftAddr][_tokenID], "This NFT is already staked.");
        stakeInfo[msg.sender].push(StakeInfo(_collectionID,nftAddr,_tokenID,block.timestamp,nftHashpower[nftAddr],block.timestamp,true));
        userData[msg.sender].hashpower += nftHashpower[nftAddr];
        totalHashpower += nftHashpower[nftAddr];
        stakedNFT[nftAddr][_tokenID] = true;
        NFT(nftAddr).transferFrom(msg.sender, address(this), _tokenID);
        emit Stake(msg.sender, nftAddr, _tokenID, block.timestamp);
    }

    function batchStake(uint256 _collectionID, uint256[] memory _tokenIDs) external nonReentrant {
        address nftAddr = nftContract[_collectionID];
        require(nftAddr != address(0), "Cannot be null address.");
        uint256 hashPowerIncrease = nftHashpower[nftAddr]*_tokenIDs.length;
        userData[msg.sender].hashpower += hashPowerIncrease;
        totalHashpower += hashPowerIncrease;
        uint256 tokensCount = _tokenIDs.length;
        for (uint256 i; i < tokensCount;) {
            require(!stakedNFT[nftAddr][_tokenIDs[i]], "One of the NFTs is already staked.");
            stakeInfo[msg.sender].push(StakeInfo(_collectionID,nftAddr,_tokenIDs[i],block.timestamp,nftHashpower[nftAddr],block.timestamp,true));
            stakedNFT[nftAddr][_tokenIDs[i]] = true;
            NFT(nftAddr).transferFrom(msg.sender, address(this), _tokenIDs[i]);
            unchecked {
                ++i;
            }
        }
        emit BatchStake(msg.sender, nftAddr, _tokenIDs, block.timestamp);
    }

    function unstake(uint256 _collectionID, uint256 _tokenID) external nonReentrant {
        uint256 hashpowerDecrease;
        uint256 stakingRecords = stakeInfo[msg.sender].length;
        address nftAddr = nftContract[_collectionID];
        for (uint256 i; i < stakingRecords;) {
            if (stakeInfo[msg.sender][i].tokenID == _tokenID && stakeInfo[msg.sender][i].collectionID == _collectionID && stakeInfo[msg.sender][i].staked) {
                uint256 mcdMined = getTokenYield(msg.sender, _collectionID, _tokenID);
                if (block.timestamp >= stakeInfo[msg.sender][i].stakeTime + vestingPeriod) {
                    uint256 earnedMCD = mcdMined * mcdPercent / divider;
                    uint256 earnedXMCD = mcdMined - earnedMCD;
                    userData[msg.sender].xMCD += earnedXMCD;
                    IERC20(mcd).safeTransfer(msg.sender, earnedMCD);
                } else {
                    IERC20(mcd).safeTransfer(treasury, mcdMined);
                }
                stakeInfo[msg.sender][i].lastClaimTime = block.timestamp;
                stakeInfo[msg.sender][i].staked = false;
                userData[msg.sender].hashpower -= stakeInfo[msg.sender][i].hashpower;
                hashpowerDecrease += stakeInfo[msg.sender][i].hashpower;
                stakedNFT[nftAddr][_tokenID] = false;
                NFT(nftAddr).transferFrom(address(this), msg.sender, _tokenID);
                break;
            }
            unchecked {
                ++i;
            }
        }
        totalHashpower -= hashpowerDecrease;
        emit Unstake(msg.sender, nftAddr, _tokenID, block.timestamp);
    }

    function claimSingleNFTYield(uint256 _collectionID, uint256 _tokenID) external nonReentrant {
        uint256 stakingRecords = stakeInfo[msg.sender].length;
        for (uint256 i; i < stakingRecords;) {
            if (stakeInfo[msg.sender][i].tokenID == _tokenID && stakeInfo[msg.sender][i].collectionID == _collectionID && stakeInfo[msg.sender][i].staked) {
                require(block.timestamp >= stakeInfo[msg.sender][i].lastClaimTime + timeStep, "Not yet 1 day since last claim.");
                if (block.timestamp >= stakeInfo[msg.sender][i].stakeTime + vestingPeriod) {
                    uint256 mcdMined = getTokenYield(msg.sender, _collectionID, _tokenID);
                    require(mcdMined > 0, "Nothing to be claimed.");
                    uint256 earnedMCD = mcdMined * mcdPercent / divider;
                    uint256 earnedXMCD = mcdMined - earnedMCD;
                    userData[msg.sender].xMCD += earnedXMCD;
                    stakeInfo[msg.sender][i].lastClaimTime = block.timestamp;
                    IERC20(mcd).safeTransfer(msg.sender, earnedMCD);
                }
                break;
            }
            unchecked {
                ++i;
            }
        }
        emit ClaimSingleNFTYield(msg.sender, nftContract[_collectionID], _tokenID, block.timestamp, getTokenYield(msg.sender, _collectionID, _tokenID));
    }

    function claimAll() external nonReentrant {
        uint256 totalYield;
        uint256 stakingRecords = stakeInfo[msg.sender].length;
        for (uint256 i; i < stakingRecords;) {
            if (stakeInfo[msg.sender][i].staked && block.timestamp >= stakeInfo[msg.sender][i].stakeTime + vestingPeriod) {
                if (block.timestamp >= stakeInfo[msg.sender][i].lastClaimTime + timeStep) {
                    uint256 mcdMined = getTokenYield(msg.sender, stakeInfo[msg.sender][i].collectionID, stakeInfo[msg.sender][i].tokenID);
                    totalYield += mcdMined;
                    stakeInfo[msg.sender][i].lastClaimTime = block.timestamp;
                }
            }
            unchecked {
                ++i;
            }
        }
        require(totalYield > 0, "Nothing to be claimed.");
        uint256 earnedMCD = totalYield * mcdPercent / divider;
        uint256 earnedXMCD = totalYield - earnedMCD;
        userData[msg.sender].xMCD += earnedXMCD;
        IERC20(mcd).safeTransfer(msg.sender, earnedMCD);
        emit ClaimAll(msg.sender, block.timestamp, earnedMCD, earnedXMCD);
    }

    function getTokenYield(address _wallet, uint256 _collectionID, uint256 _tokenID) public view returns (uint256) {
        uint256 yield;
        uint256 stakingRecords = stakeInfo[_wallet].length;
        for (uint256 i; i < stakingRecords;) {
            if (stakeInfo[_wallet][i].tokenID == _tokenID && stakeInfo[_wallet][i].collectionID == _collectionID && stakeInfo[_wallet][i].staked) {
                uint256 stakedPeriod = block.timestamp - stakeInfo[_wallet][i].lastClaimTime;
                uint256 fullDayCount = stakedPeriod/timeStep;
                yield = (tokenEmission*stakeInfo[_wallet][i].hashpower*fullDayCount/totalHashpower/30);
                break;
            }
            if (i >= 20000) break;
            unchecked {
                ++i;
            }
        }
        return yield;
    }

    function getTotalTokenYield(address _wallet) public view returns (uint256) {
        uint256 totalYield = 0;
        uint256 stakingRecords = stakeInfo[_wallet].length;
        for (uint256 i; i < stakingRecords;) {
            if (stakeInfo[_wallet][i].staked) {
                uint256 stakedPeriod = block.timestamp - stakeInfo[_wallet][i].lastClaimTime;
                uint256 fullDayCount = stakedPeriod/timeStep;
                totalYield += (tokenEmission*stakeInfo[_wallet][i].hashpower*fullDayCount/totalHashpower/30);
            }
            if (i >= 20000) break;
            unchecked {
                ++i;
            }
        }
        return totalYield;
    }

    function getTotalClaimable(address _wallet) public view returns (uint256) {
        uint256 totalClaimable = 0;
        uint256 stakingRecords = stakeInfo[_wallet].length;
        for (uint256 i; i < stakingRecords;) {
            if (stakeInfo[_wallet][i].staked && block.timestamp >= stakeInfo[msg.sender][i].stakeTime + vestingPeriod) {
                uint256 stakedPeriod = block.timestamp - stakeInfo[_wallet][i].lastClaimTime;
                uint256 fullDayCount = stakedPeriod/timeStep;
                totalClaimable += (tokenEmission*stakeInfo[_wallet][i].hashpower*fullDayCount/totalHashpower/30);
            }
            if (i >= 20000) break;
            unchecked {
                ++i;
            }
        }
        return totalClaimable;
    }

    function getCurDay() public view returns (uint256) {
        return (block.timestamp - stakingStart) / timeStep;
    }

    function transferXMCD(address _receiver, uint256 _transferAmount) external {
        require(_receiver != address(0), "Receiver cannot be null address.");
        require(userData[msg.sender].xMCD >= _transferAmount, "User insufficient xMCD balance.");
        userData[msg.sender].xMCD -= _transferAmount;
        userData[_receiver].xMCD += _transferAmount;
        emit TransferXMCD(msg.sender, _receiver, _transferAmount, block.timestamp);
    }

    function debitXMCD(address _spender, uint256 _debitAmount) external {
        require(admin[msg.sender] || msg.sender == owner(), "Only admin or owner can execute.");
        require(_spender != address(0), "Spender cannot be null address.");
        require(_debitAmount > 0, "Debit amount cannot be zero.");
        require(userData[_spender].xMCD >= _debitAmount, "User insufficient xMCD balance.");
        userData[_spender].xMCD -= _debitAmount;
        emit DebitXMCD(msg.sender, _debitAmount, block.timestamp);
    }

    function creditXMCD(address _receiver, uint256 _creditAmount) external {
        require(admin[msg.sender] || msg.sender == owner(), "Only admin or owner can execute.");
        require(_receiver != address(0), "Receiver cannot be null address.");
        require(_creditAmount > 0, "Credit amount cannot be zero.");
        userData[_receiver].xMCD += _creditAmount;
        emit CreditXMCD(msg.sender, _receiver, _creditAmount, block.timestamp);
    }

    function genesisStakeMigrate(address _wallet, uint256 _tokenID, uint256 _startTime, uint256 _lastClaim) external {
        require(admin[msg.sender] || msg.sender == owner(), "Only admin or owner can execute.");
        require(_wallet != address(0), "Cannot migrate to null address.");
        require(!stakedNFT[nftContract[0]][_tokenID], "This NFT is already staked.");
        require(_startTime > 0 && _lastClaim > 0, "Timestamp cannot be zero.");
        require(_startTime >= stakingStart, "Timestamp must be greater than staking start time.");
        require(_lastClaim >= _startTime, "Invalid last claim timestamp.");
        stakeInfo[_wallet].push(StakeInfo(0,nftContract[0],_tokenID,_startTime,nftHashpower[nftContract[0]],_lastClaim,true));
        userData[_wallet].hashpower += nftHashpower[nftContract[0]];
        totalHashpower += nftHashpower[nftContract[0]];
        stakedNFT[nftContract[0]][_tokenID] = true;
        emit GenesisStakeMigrate(msg.sender, _wallet, _tokenID, _startTime, _lastClaim);
    }

    function genesisStakeBatchMigrate(address[] memory _wallet, uint256[] memory _tokenIDs, uint256 _startTime, uint256 _lastClaim) external {
        require(admin[msg.sender] || msg.sender == owner(), "Only admin or owner can execute.");
        require(_wallet.length == _tokenIDs.length, "Unmatched data for wallet and token IDs.");
        require(_startTime > 0 && _lastClaim > 0, "Timestamp cannot be zero.");
        require(_startTime >= stakingStart, "Timestamp must be greater than staking start time.");
        require(_lastClaim >= _startTime, "Invalid last claim timestamp.");
        uint256 hashpowerIncreaseCount;
        uint256 walletCount = _wallet.length;
        for (uint256 i; i < walletCount;) {
            if (_wallet[i] != address(0)) {
                require(!stakedNFT[nftContract[0]][_tokenIDs[i]], "One of the NFTs is already staked.");
                stakeInfo[_wallet[i]].push(StakeInfo(0,nftContract[0],_tokenIDs[i],_startTime,nftHashpower[nftContract[0]],_lastClaim,true));
                userData[_wallet[i]].hashpower += nftHashpower[nftContract[0]];
                hashpowerIncreaseCount++;
                stakedNFT[nftContract[0]][_tokenIDs[i]] = true;
            }
            unchecked {
                ++i;
            }
        }
        totalHashpower += (nftHashpower[nftContract[0]]*hashpowerIncreaseCount);
        emit GenesisStakeBatchMigrate(msg.sender, _wallet, _tokenIDs, _startTime, _lastClaim);
    }

    function genesisTokenMigrate(address _wallet, uint256 _amount) external {
        require(admin[msg.sender] || msg.sender == owner(), "Only admin or owner can execute.");
        require(_wallet != address(0), "Cannot migrate to null address.");
        genesisMigratedToken[_wallet] = _amount;
        emit GenesisTokenMigrate(msg.sender, _wallet, _amount);
    }

    function genesisTokenBatchMigrate(address[] memory _wallet, uint256[] memory _amount) external {
        require(admin[msg.sender] || msg.sender == owner(), "Only admin or owner can execute.");
        require(_wallet.length == _amount.length, "Unmatched data for wallet and amount.");
        uint256 walletCount = _wallet.length;
        for (uint256 i; i < walletCount;) {
            if (_wallet[i] != address(0)) {
                genesisMigratedToken[_wallet[i]] = _amount[i];
            }
            unchecked {
                ++i;
            }
        }
        emit GenesisTokenBatchMigrate(msg.sender, _wallet, _amount);
    }

    function genesisTokenClaim() external nonReentrant {
        require(genesisMigratedToken[msg.sender] > 0, "No migrated token can be claimed.");
        require(block.timestamp >= genesisMigrationDate + genesisLockPeriod, "Migrated tokens still under locking period.");
        uint256 unlockedMCD = genesisMigratedToken[msg.sender] * mcdPercent / divider;
        uint256 earnedXMCD = genesisMigratedToken[msg.sender] - unlockedMCD;
        userData[msg.sender].xMCD += earnedXMCD;
        genesisMigratedToken[msg.sender] = 0;
        IERC20(mcd).safeTransfer(msg.sender, unlockedMCD);
        emit GenesisTokenClaim(msg.sender, unlockedMCD, earnedXMCD, block.timestamp);
    }

    function releaseToTreasury() external nonReentrant {
        require(block.timestamp >= lastLiquidityRelease + timeStep, "Not yet 24 hours since last released.");
        require(totalHashpower > 0, "No NFT is staked for mining.");
        uint256 releasableDays = (block.timestamp-lastLiquidityRelease)/timeStep;
        uint256 releasableMCD = tokenEmission*releasableDays*xmcdPercent/divider/30;
        lastLiquidityRelease += (releasableDays*timeStep);
        IERC20(mcd).safeTransfer(treasury, releasableMCD);
        emit ReleaseToTreasury(msg.sender, releasableDays, releasableMCD, block.timestamp);
    }

    function setTokenEmission(uint256 _newEmission) external {
        require(admin[msg.sender] || msg.sender == owner(), "Only admins or owner can execute.");
        tokenEmission = _newEmission;
        emit SetTokenEmission(msg.sender, _newEmission, block.timestamp);
    }

    function addNFTContract(address _contract, uint256 _power) external onlyOwner {
        require(_contract != address(0), "Cannot add null address.");
        require(_power > 0, "Power cannot be zero.");
        nftContract[totalCollection] = _contract;
        nftHashpower[_contract] = _power;
        totalCollection++;
        emit AddNFTContract(msg.sender, totalCollection, _contract, block.timestamp);
    }

    function manageAdmin(address _adminAddr, bool _state) external onlyOwner {
        require(_adminAddr != address(0), "Cannot add null address.");
        admin[_adminAddr] = _state;
        emit ManageAdmin(_adminAddr, _state, block.timestamp);
    }

    function updateNFTHashpower(address _nftContract, uint256 _power) external onlyOwner {
        require(_nftContract != address(0), "Invalid contract address.");
        uint256 oldPower = nftHashpower[_nftContract];
        nftHashpower[_nftContract] = _power;
        emit UpdateNFTHashpower(msg.sender, oldPower, _power, block.timestamp);
    }

    function setGenesisContractAddr(address _genesisContract) external onlyOwner {
        require(_genesisContract == address(0), "Genesis contract already set.");
        nftContract[0] = _genesisContract;
        nftHashpower[nftContract[0]] = 100;
        emit SetGenesisContractAddr(msg.sender, _genesisContract, block.timestamp);
    }

    function getUserStakingInfo(address _userAddr) external view returns(StakeInfo[] memory) {
        return stakeInfo[_userAddr];
    }

    function getUserData(address _userAddr) external view returns(UserData memory) {
        return userData[_userAddr];
    }

}