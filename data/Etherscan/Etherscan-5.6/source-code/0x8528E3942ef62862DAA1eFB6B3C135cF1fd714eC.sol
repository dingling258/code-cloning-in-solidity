    "contracts/MorePufferPoints.sol": {
      "content": "// SPDX-License-Identifier: GPL-3.0\r\npragma solidity ^0.8.25;\r\n\r\nimport {IERC20} from \"@openzeppelin/contracts/token/ERC20/IERC20.sol\";\r\nimport \"@openzeppelin/contracts/access/Ownable.sol\";\r\nimport {IERC4626} from \"@openzeppelin/contracts/interfaces/IERC4626.sol\";\r\nimport {IWstETH} from \"./interfaces/IWstETH.sol\";\r\nimport {FlashLoanSimpleReceiverBase} from \"@aave/core-v3/contracts/flashloan/base/FlashLoanSimpleReceiverBase.sol\";\r\nimport {IPoolAddressesProvider} from \"@aave/core-v3/contracts/interfaces/IPoolAddressesProvider.sol\";\r\nimport {ICurvePool} from \"./interfaces/ICurvePool.sol\";\r\n\r\n/**\r\n * @title MorePufferPoints\r\n * @author Danny Chyou\r\n */\r\ncontract MorePufferPoints is FlashLoanSimpleReceiverBase, Ownable {\r\n    address private immutable pufferVaultAddress =\r\n        0xD9A442856C234a39a81a089C06451EBAa4306a72;\r\n    address private immutable stETHAddress =\r\n        0xae7ab96520DE3A18E5e111B5EaAb095312D7fE84;\r\n    address private immutable wstETHAddress =\r\n        0x7f39C581F595B53c5cb19bD0b3f8dA6c935E2Ca0;\r\n    address private immutable wstETH2pufETHPoolAddress =\r\n        0xEEda34A377dD0ca676b9511EE1324974fA8d980D;\r\n    uint256 public constant PROTOCOL_FEE = 5; // 0.05%\r\n    uint256 public totalProtocolFeeAmount = 0;\r\n\r\n    event Log(\r\n        address sender,\r\n        uint256 loanAmount,\r\n        uint256 stETHAmount,\r\n        uint256 pufETHAmount,\r\n        uint256 outputWstETH,\r\n        uint256 debt\r\n    );\r\n\r\n    constructor(\r\n        address _poolAddressProvider\r\n    )\r\n        FlashLoanSimpleReceiverBase(\r\n            IPoolAddressesProvider(_poolAddressProvider)\r\n        )\r\n        Ownable(msg.sender)\r\n    {}\r\n\r\n    function runWstETH(uint256 inputAmount, uint256 loanAmount) external {\r\n        bytes memory params = abi.encode(inputAmount);\r\n        POOL.flashLoanSimple(\r\n            address(this),\r\n            wstETHAddress,\r\n            loanAmount,\r\n            params,\r\n            0\r\n        );\r\n    }\r\n\r\n    function executeOperation(\r\n        address asset,\r\n        uint256 amount,\r\n        uint256 premium,\r\n        address initiator,\r\n        bytes calldata params\r\n    ) external returns (bool) {\r\n        uint256 inputAmount = abi.decode(params, (uint256));\r\n        address receiver = tx.origin;\r\n\r\n        require(initiator == address(this), \"II\"); // Invalid initiator\r\n        require(asset == wstETHAddress, \"IA\"); // Invalid asset\r\n\r\n        IERC20(wstETHAddress).approve(address(POOL), amount + premium);\r\n\r\n        uint256 stETHAmount = unwrapStETH(amount);\r\n\r\n        uint256 pufETHAmount = depositToPuffer(stETHAmount, receiver);\r\n\r\n        takeBackPufEth(pufETHAmount, receiver);\r\n\r\n        uint256 outputWstETH = swap(pufferVaultAddress, pufETHAmount);\r\n        uint256 protocolFee = (amount * PROTOCOL_FEE) / 10000;\r\n        uint256 debt = amount + premium - outputWstETH + protocolFee;\r\n\r\n        require(debt <= inputAmount, \"MLR\"); // Max loss rate\r\n\r\n        IERC20(wstETHAddress).transferFrom(\r\n            receiver,\r\n            address(this),\r\n            debt\r\n        );\r\n\r\n        totalProtocolFeeAmount += protocolFee;\r\n\r\n        emit Log(\r\n            receiver,\r\n            amount,\r\n            stETHAmount,\r\n            pufETHAmount,\r\n            outputWstETH,\r\n            debt\r\n        );\r\n\r\n        return true;\r\n    }\r\n\r\n    function unwrapStETH(uint amount) internal returns (uint256) {\r\n        return IWstETH(wstETHAddress).unwrap(amount);\r\n    }\r\n\r\n    function depositToPuffer(\r\n        uint amount,\r\n        address receiver\r\n    ) internal returns (uint256) {\r\n        IERC4626 pufferVault = IERC4626(pufferVaultAddress);\r\n        IERC20(stETHAddress).approve(pufferVaultAddress, amount);\r\n        return pufferVault.deposit(amount, receiver);\r\n    }\r\n\r\n    function takeBackPufEth(uint amount, address receiver) internal {\r\n        IERC20(pufferVaultAddress).transferFrom(\r\n            receiver,\r\n            address(this),\r\n            amount\r\n        );\r\n    }\r\n\r\n    function swap(address input, uint amount) internal returns (uint256) {\r\n        IERC20(input).approve(wstETH2pufETHPoolAddress, amount);\r\n        return ICurvePool(wstETH2pufETHPoolAddress).exchange(0, 1, amount, 0);\r\n    }\r\n\r\n    function withdrawProtocolFee() external onlyOwner {\r\n        IERC20(wstETHAddress).transfer(owner(), totalProtocolFeeAmount);\r\n        totalProtocolFeeAmount = 0;\r\n    }\r\n}\r\n"
    }
